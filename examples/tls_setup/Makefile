#ECCConsole

WORKDIR = `pwd`


INC = -I../include  -I../g3_api_library/include -I../g3_io_lib
CFLAGS = -Wall -fexceptions  -O2
RESINC = 
LIBDIR = 




INC_RELEASE = $(INC)
CFLAGS_RELEASE = $(CFLAGS) -D_DEFNEODEF_ -D__USE_CDC__0  -DNEODEBUG  -D__USE_FT4222__ $(TEMPDEF)
RESINC_RELEASE = $(RESINC)
RCFLAGS_RELEASE = $(RCFLAGS)
LIBDIR_RELEASE = $(LIBDIR)

LIB_RELEASE = $(LIB)

LDFLAGS_RELEASE = $(LDFLAGS) -s 
OBJDIR_RELEASE = $(OBJDIR)
DEP_RELEASE = 
OUT_RELEASE = $(OUTDIR)/tls_setup

OBJ_RELEASE = 	$(OBJDIR_RELEASE)/main.o \
				$(OBJDIR_RELEASE)/util.o \
				$(OBJDIR_RELEASE)/tls_setup.o  

					
	
		

all: clean release

clean: clean_debug clean_release


before_release: 
	test -d $(OUTDIR) || mkdir -p $(OUTDIR)
	test -d $(OBJDIR_RELEASE)/src || mkdir -p $(OBJDIR_RELEASE)/src

after_release: 

release: before_release out_release after_release

out_release: before_release $(OBJ_RELEASE) $(DEP_RELEASE)
	$(LD) $(LIBDIR_RELEASE) -o $(OUT_RELEASE) $(OBJ_RELEASE)  $(LDFLAGS_RELEASE) $(LIB_RELEASE)
	
$(OBJDIR_RELEASE)/%.o: %.cpp
	$(CXX) $(CFLAGS_RELEASE) $(INC_RELEASE)  -c -o $@ $<
	




clean_release: 
	rm -f $(OBJ_RELEASE) $(OUT_RELEASE)
	rm -rf bin/Release
	rm -rf $(OBJDIR_RELEASE)/src

.PHONY: before_debug after_debug clean_debug before_release after_release clean_release





