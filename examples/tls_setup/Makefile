CXX = g++
AR = ar
LD = g++
WINDRES = windres
OUTDIR = ../bin/gnu
OBJDIR = obj/gnu
LIB = -L../lib/gnu -lg3_api_lib -lg3_io_lib ../lib/gnu/libneolib_c_base.a -lwiringPi -ldl -lft4222
